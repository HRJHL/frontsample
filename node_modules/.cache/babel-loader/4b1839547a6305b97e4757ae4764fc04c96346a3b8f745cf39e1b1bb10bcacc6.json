 this.customerName = name;\n  },\n  methods: {\n    async fetchCredits() {\n  try {\n    const response = await axios.get(`/api/credits/${this.customerName}`);\n    this.credits = response.data.data;\n  } catch (error) {\n    console.error(error);\n    this.error = 'Error fetching credit information.';\n  } finally {\n    this.loading = false;\n  }\n}\n\n  }\n};\n</script>\n\n<style scoped>\n@media (max-width: 768px) {\n  .conbox {\n    margin-left: 0px;\n    width: 100%;\n    overflow-x: scroll;\n    overflow: auto;\n    white-space: nowrap;\n  }\n}\n</style>\n"],"mappings":"AAmBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,IAAI;MACXC,YAAY,EAAE,EAAC,CAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;IACnB,IAAIP,IAAG,GAAIQ,cAAc,CAACC,OAAO,CAAC,MAAM,CAAC;IACzC,IAAI,CAACJ,YAAW,GAAIL,IAAI;EAC1B,CAAC;EACDU,OAAO,EAAE;IACP,MAAMH,YAAYA,CAAA,EAAG;MACvB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,gBAAgB,IAAI,CAACP,YAAY,EAAE,CAAC;QACrE,IAAI,CAACH,OAAM,GAAIS,QAAQ,CAACV,IAAI,CAACA,IAAI;MACnC,EAAE,OAAOG,KAAK,EAAE;QACdS,OAAO,CAACT,KAAK,CAACA,KAAK,CAAC;QACpB,IAAI,CAACA,KAAI,GAAI,oCAAoC;MACnD,UAAU;QACR,IAAI,CAACD,OAAM,GAAI,KAAK;MACtB;IACF;EAEE;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}